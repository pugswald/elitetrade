    height:30px
    line-height:30px
    margin:0 3px 3px 0
    //vertical-align:center
    //vertical-align: text-bottom
    img
      //display: inline-block
      height: 22px
      width: 22px
      padding:0 3px
      margin:0
      vertical-align:text-bottom
    .remove-friend
      padding:0 3px
      font-weight:bold
      display:inline-block
      
